<?php

function drupal_test_support_mail_mail($key, &$message): void
{
    if ($key !== 'drupal_test_support_mail') {
        return;
    }

    $message['subject'] = \Drupal::state()->get('drupal_test_support.mail_subject');
    $message['body'][] = \Drupal::state()->get('drupal_test_support.mail_body');
}
