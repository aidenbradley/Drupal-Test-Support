{
    "name": "aidenbradley/test-support",
    "description": "Provides various traits to help test setup and assertions in Drupal Kernel tests",
    "type": "drupal-module",
    "require": {
        "php": ">=5.3.0",
        "drupal/core-dev": "^9.4",
        "phpspec/prophecy-phpunit": "^2.0",
        "phpunit/phpunit": "^9.5",
        "illuminate/collections": "^8.77",
        "opis/closure": "^3.6",
        "nesbot/carbon": "2.x-dev"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Aiden Bradley",
            "email": "mraidenbradley@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
