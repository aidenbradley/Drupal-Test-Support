{
    "name": "aidenbradley/test-support",
    "description": "Provides various traits to help test setup and assertions in Drupal Kernel tests",
    "type": "drupal-module",
    "require": {
        "php": ">=5.3.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Aiden Bradley",
            "email": "mraidenbradley@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "illuminate/collections": "^8.77",
        "opis/closure": "^3.6",
        "nesbot/carbon": "2.x-dev",
        "symfony/phpunit-bridge": "5.4.x-dev",
        "drupal/core-dev": "^9.5",
        "phpspec/prophecy-phpunit": "^1.1",
        "brianium/paratest": "^0.4.3"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        },
        "platform": {
            "php": "7.3"
        }
    }
}
